&huawei_audio_info {
	compatible = "hw,hw_audio_info";
	product_identifier = "mrr";
	pa_identifier = "smartpakit";
	smartpa_num = "4";
	voice_vol_level = "10";
	status = "ok";
	speaker_test_content = "leftspeaker,rightspeaker,subleftspeaker,subrightspeaker";
