&dsi_dbr_110_a09_10p95_qcom {
	qcom,mdss-dsi-pan-enable-dynamic-fps;
	qcom,dsi-supported-dfps-list = <60 120>;
	qcom,mdss-dsi-pan-fps-update = "dfps_immediate_porch_mode_vfp";
	qcom,mdss-dsi-display-timings {
		timing@0 {
			qcom,mdss-dsi-to-60-cmds = [39 00 00 00 00 00 04 FF 98 82 02
					39 01 00 00 00 00 02 1B 01];
			qcom,mdss-dsi-to-60-cmds-status = "dsi_lp_mode";
			qcom,mdss-dsi-to-120-cmds = [39 00 00 00 00 00 04 FF 98 82 02
					39 01 00 00 00 00 02 1B 00];
			qcom,mdss-dsi-to-120-cmds-status = "dsi_hs_mode";
		};
	};
};

