&water_check {
	pinctrl-names = "water_check_irq_pins_ear",
			"water_check_irq_pins_sim",
			"water_check_irq_pins_key";
	pinctrl-0 = <&water_check_irq_pins_ear>;
	pinctrl-1 = <&water_check_irq_pins_sim>;
	pinctrl-2 = <&water_check_irq_pins_key>;
	gpio_ear = <&tlmm 96 0>; /* gpio_096 */
	gpio_sim = <&tlmm 104 0>; /* gpio_104 */
	gpio_key = <&tlmm 103 0>; /* gpio_103 */
	/* check_type, gpio_name, irq_no, multiple_handle, dmd_offset, prompt, action */
	water_check_para =
		"1", "ear", "-1", "1", "1", "0", "0",
		"1", "sim", "-1", "1", "2", "0", "0",
		"1", "key", "-1", "1", "3", "0", "0",
		"2", "null","-1", "0", "4", "0", "0",
		"3", "null","-1", "0", "5", "0", "0";
	irq_trigger_type = <1>; /* 1:low level trigger, 0:edge trigger */
	pinctrl_enable = <1>; /* 1:use pinctrl enable gpio pins state, 0:others */
	status = "disabled";
};

