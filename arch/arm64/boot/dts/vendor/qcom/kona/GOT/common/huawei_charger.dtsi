&tlmm {
	tx_ic_irq_pins_default: tx_ic_irq_pins_default {
		mux {
			pins = "gpio129";
			function = "gpio";
		};
		config {
			pins = "gpio129";
			bias-pull-up;
			input-enable;
		};
	};

	tx_ic_en_pins_default: tx_ic_en_pins_default {
		pins = "gpio92";
		function = "gpio";
		output-high;
		bias-pull-up;
	};

	wireless_aux_tx_pen_pins_default: wireless_aux_tx_pen_pins_default {
		mux {
			pins = "gpio100";
			function = "gpio";
		};
		config {
			pins = "gpio100";
                        drive-strength = <2>;
                        bias-pull-up;
                        input-enable;
		};
	};

	wireless_aux_tx_kb_pins_default: wireless_aux_tx_kb_pins_default {
		mux {
			pins = "gpio135";
			function = "gpio";
		};
		config {
			pins = "gpio135";
			drive-strength = <2>;
			bias-pull-down;
			input-enable;
		};
	};

	bq25970_irq_pins_default: bq25970_irq_pins_default {
		mux {
			pins = "gpio51";
			function = "gpio";
		};
		config {
			pins = "gpio51";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};

	fsa9685_irq_pins_default: fsa9685_irq_pins_default {
		mux {
			pins = "gpio67";
			function = "gpio";
		};
		config {
			pins = "gpio67";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};

	fsa9685_switch_pins_default: fsa9685_switch_pins_default {
		mux {
			pins = "gpio175";
			function = "gpio";
		};
		config {
                        pins = "gpio175";
                        drive-strength = <2>;
                        bias-pull-up;
                        output-high;
		};
	};

	usb_sw_oe_default: usb_sw_oe_default{
		mux {
			pins = "gpio137";
			function = "gpio";
		};
		config {
			pins = "gpio137";
			drive-strength = <2>;
			bias-pull-down;
			intput-enable;
		};
	};

	tx_boost_en_pins_default: tx_boost_en_pins_default {
		mux {
			pins = "gpio50";
			function = "gpio";
		};
		config {
			pins = "gpio50";
			drive-strength = <2>;
			bias-pull-down;
			input-enable;
		};
	};

	water_check_irq_pins_ear: water_check_irq_pins_ear {
		pins = "gpio96";
		function = "gpio";
		bias-pull-up;
	};
	water_check_irq_pins_sim: water_check_irq_pins_sim {
		pins = "gpio104";
		function = "gpio";
		bias-pull-up;
	};
	water_check_irq_pins_key: water_check_irq_pins_key {
		pins = "gpio103";
		function = "gpio";
		bias-pull-up;
	};

	gpio_otg_switch_pin_default: gpio_otg_switch_pin_default {
		mux {
			pins = "gpio100";
			function = "gpio";
		};
		config {
			pins = "gpio100";
			drive-strength = <2>;
			bias-pull-down;
			input-enable;
		};
	};
	battct_swi_pins_default: battct_swi_pins_default {
		mux {
			pins = "gpio25";
			drive-strength = <6>;
			function = "gpio";
		};
		config {
			pins = "gpio25";
			input-enable;
		};
	};
};

