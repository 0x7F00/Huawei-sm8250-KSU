&qupv3_se15_i2c {
	#address-cells = <1>;
	#size-cells = <0>;
	qcom,clk-freq-out = <400000>; /* if bq27z561_fg is used, i2c clock must be 100k(100000) */
	status = "ok";
	fairchild_fsa9685@25 {
		compatible = "huawei,fairchild_fsa9685";
		reg = <0x25>;
		pinctrl-names = "default", "switch_gpio_init", "default";
		pinctrl-0 = <&fsa9685_irq_pins_default>;
		pinctrl-1 = <&fsa9685_switch_pins_default>;
		pinctrl-2 = <&usb_sw_oe_default>;
		fairchild_fsa9685,gpio-intb = <&tlmm 67 0>;
		switch_gpio = <&tlmm 175 0>;
		usb_sw_oe_gpio = <&tlmm 137 0>;
		usbid-enable = <0>;
		fcp_support = <1>;
		scp_support = <1>;
		mhl_detect_disable = <1>;
		need_other_bc12 = <0>;
		ignore_force_ac = <1>;
		/* for rt8979 */
		interrupt-names = "vbus_change", "device_change", "detach",
		              "attach";
		/* auto_switch; */
		scp;
		dcd;
		dcd_timeout = <600>;  /* ms */
		intb_watchdog = <1000>;  /* ms */
		ovp_sel = <14500000>;  /* uV */
		status = "ok";
	};
};

